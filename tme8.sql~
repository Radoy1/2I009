create table Employe(
numss number(5),
nomE varchar2(20),
prenomE varchar2(20),
villeE varchar2(20),
datnaiss date,
datecour date default sysdate,
constraint pk_emp primary key(numss),
constraint format_uss check(lenght(numss)=15),
constraint ville_emp check (lower(villeE) in ('paris', 'lyon', 'marseille'))
constraint limite_age check((datecours-datenaiss)/365 <70));

